#ex1
import math
degree = int(input())
print(degree * (math.pi / 180))

#ex2
height = int(input())
base_first = int(input())
base_second = int(input())
print(((base_first + base_second)/2)*height)

#ex3
import math
bases = int(input())
length = int(input())
print((bases/4)*(length**2)*(1/math.tan(math.pi/bases)))

#ex4
height = int(input())
base = int(input())
print(height*base)
