#ex1
def grams_to(grams):
    return 28.3495231 * grams
grams_amount = float(input("Enter the amount in grams: "))
ounce_result = grams_to(grams_amount)
print(ounce_result)

#ex2
def temp(f):
    C = (5 / 9) * (f - 32)
    return C
f = float(input())
print(temp(f))

#ex3
def math(head, leg):
    rab = leg / 2 - head
    chick = head - rab
    return int(rab), int(chick)
head = int(input())
leg = int(input())
print(math(head, leg))

